 		talktime = "30 seconds" //this sets up the envelope properly
		aitalkinterval = 30 //this changes the actual rate
		
 		"\0\s[12]Feeling chatty, are we?\w8\n\s[17]No problem. I could talk to you for days~\e"
	}

//If the user chooses every minute.
Select.TALKHIGH
	{
 		talktime = "minute"
		aitalkinterval = 60
		
 		"\0\s[12]Am I that interesting?\n\w8\s[13]Or do you just like hearing my voice.\n\w8\s[37]Either way, I'm happy to.\e"
	}

//If the user chooses every three minutes.
Select.TALKNORMAL
